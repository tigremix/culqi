<script type="text/javascript">
    (function () {
        var header = document.querySelector("#navsticky");
        if (window.location.hash) {
            header.classList.add("headroom--unpinned");
        }
        var headroom = new Headroom(header, {
            tolerance: {
                down: 10
                , up: 20
            }
            , offset: 205, // callback when pinned, `this` is headroom object
            /*onPin : function() { $( "#home .contentallmenu" ).addClass('pin');
            $( "#home .contentallmenu" ).removeClass('unpin');  },
            // callback when unpinned, `this` is headroom object
            onUnpin : function() { $( "#home .contentallmenu" ).addClass('unpin');
            $( "#home .contentallmenu" ).removeClass('pin');   }*/
        });
        headroom.init();
    }());
</script>
<script type="text/javascript">
$(document).ready(function (e) {
    $('.otros').click(function () {
        $(".listamenu > ul").find("li").each(function () {
            $(this).find("a").removeClass("seleccionado");
        });
        $(this).addClass("seleccionado");
    });
    //Veamos la resolucion de pantalla
});
</script>